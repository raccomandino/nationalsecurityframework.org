{
  "rewrites": [
    {
      "source": "/polarpound/_next/:path*",
      "destination": "https://nationalsecurityframework-org.vercel.app/_next/:path*"
    },
    {
      "source": "/polarpound",
      "destination": "https://nationalsecurityframework-org.vercel.app"
    },
    {
      "source": "/polarpound/:path*",
      "destination": "https://nationalsecurityframework-org.vercel.app/:path*"
    },
    {
      "has": [
        {
          "type": "header",
          "key": "referer",
          "value": "/polarpound.*"
        }
      ],
      "source": "/_next/:path*",
      "destination": "https://nationalsecurityframework-org.vercel.app/_next/:path*"
    }
  ]
}
